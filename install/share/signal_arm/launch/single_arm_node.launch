<launch>
    <arg name="left_serial_port_path"         default="/dev/ttyACM0"/>
    <arg name="left_joint_states_topic"       default="/joint_states_host"/>
    <arg name="left_gripper_feedback_topic"   default="/gripper_stroke_left"/>
    <arg name="left_control_topic"            default="/arm_joint_command_host"/>
    <arg name="left_arm_status_topic"         default="/arm_status_left"/>
    <arg name="left_gripper_topic"            default="/gripper_aloha_command_left"/>
    <arg name="left_joint_names"              default="arm_joint1,arm_joint2,arm_joint3,arm_joint4,arm_joint5,arm_joint6,gripper"/>
    <arg name="left_gripper_force_control_topic" default="/gripper_force_control_left"/>
    <arg name="left_gripper_position_control_topic" default="/gripper_position_control_left"/>
    <!-- node starter-->
    <node pkg="signal_arm" type="iarm_node" name="iarm_node_host" output="screen">
        <param name="serial_port_path"              value="$(arg left_serial_port_path)"/>
        <param name="joint_states_topic"            value="$(arg left_joint_states_topic)"/>
        <param name="gripper_feedback_topic"        value="$(arg left_gripper_feedback_topic)"/>
        <param name="control_topic"                 value="$(arg left_control_topic)"/>
        <param name="arm_status_topic"              value="$(arg left_arm_status_topic)"/>
        <param name="gripper_topic"                 value="$(arg left_gripper_topic)"/>
        <param name="joint_names"                   value="$(arg left_joint_names)"/>
        <param name="gripper_force_control_topic"   value="$(arg left_gripper_force_control_topic)"/>
        <param name="gripper_position_control_topic" value="$(arg left_gripper_position_control_topic)"/>
    </node>
</launch>
